# Browser Configuration
BROWSER=chrome
HEADLESS=false
IMPLICIT_WAIT=10
EXPLICIT_WAIT=20

# Test Environment
BASE_URL=https://useinsider.com/
TEST_ENV=staging

# Selenium Grid (optional)
SELENIUM_HUB_URL=

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/test.log

# Screenshots
SCREENSHOT_ON_FAILURE=true
SCREENSHOT_DIR=screenshots

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY=2